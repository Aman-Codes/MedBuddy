                            <a class="nav-link active" data-toggle="tab" href="#" role="tab">Ambulance Registration</a>
                        </li>
                    </ul>

                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div class="tab-pane active" id="ambulanceRegistration" role="tabpanel">
                            <form action="/ambulanceregistration" method="POST" onsubmit="AddUrlList()" accept-charset="utf-8">                                                                                                     
                                <div class="form-row row mt_20">
                                <div class="form-group col-12">
                                    <p>Name</p>
                                        <input type="text" class="form-control" placeholder="Name" name="name" autocomplete="off" required>
                                    </div>
                                    <div class="form-group col-12">
                                        <div class="mb-3">
                                            <p>
                                                Please enter your address in the textarea
                                            </p>
                                            <textarea class="form-control" id="validationTextarea" placeholder="Address" name="address" autocomplete="off" required></textarea>
                                            
                                          </div>
                                    </div>
                                    <div class="form-group col-12">
                                      <label for="phonenum">Phone number</label><br/>
                                        <input id="phonenum" type="tel" class="form-control" placeholder="Phone Number (format:+91'xxxxxxxxxx')" name="phone" pattern="^\d{10}$" minlength="10" autocomplete="off" required>
                                    </div>
                                    <div class="form-group col-12">
                                        <p>Email address</p>
                                        <input type="email" class="form-control" placeholder="Email Address" name="username" autocomplete="off" required>
                                    </div>
                                    <div class="form-group col-12">
                                        <p>Set your password</p>
                                        <input type="password" class="form-control" placeholder="Password" name="password" autocomplete="off" required>
                                    </div>
                                    <div class="form-group col-12">
                                    <p>Vehicle number</p>
                                        <input type="text" class="form-control" placeholder="Vehicle Number" name="vehiclenumber" autocomplete="off" required>
                                    </div>
                                    <div class="form-group col-12">
                                        <label for="ambulancetype">Ambulance service type: </label>
                                        <select id="ambulancetype" name="ambulancetype" id="ambulancetype" required>
                                            <option value="Paid">Paid</option>
                                            <option value="Free">Free</option>
                                          </select>
                                    </div>
                                    <input type = "hidden" class = "hiddenField" name = "urllist" id = "urllist"/>
                                    <div class="form-group col-12">
                                        <p>Upload ambulance pictures</p>
                                        <p><a id="upload_widget" class="cloudinary-button" >Upload</a></p>
                                    </div>                                    

                                    <script src="https://widget.cloudinary.com/v2.0/global/all.js" type="text/javascript"></script>
                                    <script type="text/javascript">
                                    var urlList = [];  
                                    var myWidget = cloudinary.createUploadWidget({
                                    cloudName: 'dd3xrlfbk', 
                                    uploadPreset: 'sample_467a446e00ad6fc1633b185bd0262fbab46e2c9b'}, (error, result) => { 
                                        if (!error && result && result.event === "success") { 
                                        console.log('Done! Here is the image info: ', result.info.secure_url); 
                                        urlList.push(result.info.secure_url);
                                        }  
                                    })

                                    function AddUrlList (){
                                        var finalList = (urlList.toString());
                                        console.log(finalList);
                                        document.getElementById("urllist").value = finalList;

                                    }

                                    document.getElementById("upload_widget").addEventListener("click", function(){
                                        myWidget.open();
                                    }, false);
                                    </script>
                                    <div class="form-group col-12">
                                        <br><button type="submit" class="btn" name="ambulabce_registration">Register</button>
                                    </div>
                                </div>
                            </form>                       
