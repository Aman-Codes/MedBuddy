<div class="about-page pt_60 pb_30">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>Welcome to our doctor portal </h1><br>
                <h3>Hi <%= data.user.name %></h3>  <br>

                <div class="faq-group headstyle mt-30">
                    <div id="accordion">
                        <div class="faq-item card">
                            <div class="faq-header" id="heading1">
                                <button class="faq-button " data-toggle="collapse" data-target="#collapse1" aria-expanded="true" aria-controls="collapse1"><i class="fa fa-caret-right"></i> View Current Appointments </button>
                            </div>
                            <div id="collapse1" class="collapse show" aria-labelledby="heading1" data-parent="#accordion">
                                <div class="faq-body">
                                    <ol>
                                        <% if(data.futureappointment != undefined && data.futureappointment.length > 0) { %>
                                            <% data.futureappointment.forEach(function(item){ %>
                                                <div class="col-lg-12">
                                                    <li><b>Patient Name: </b><%= item.PatientName %></li> 
                                                    <b>Patient Phone Number: </b><%= item.PatientPhone %> <br/>
                                                    <b>Appointment Status: </b><%= item.Status %> <br/>
                                                    <b>Appointment Date: </b><%= item.AppointmentDate %> <br/>
                                                    <b>Appointment Time: </b><%= item.AppointmentTime %> <br/>
                                                    <a href="/doctorhome/<%= item.AppointmentId %>/accept" class="btn btn-xs btn-confirm">Confirm Appointment</a> &nbsp;
                                                    <a href="/doctorhome/<%= item.AppointmentId %>/reject" class="btn btn-xs btn-danger">Reject Appointment</a> <br/>                                                                
                                                </div>
                                                <br/>
                                            <% }); %>
                                    </ol>
                                    <% }else{ %>
                                        <p>
                                            Currently there is no appointment !!!
                                        </p>
                                    <% } %>                                          
                                </div> 
                            </div>
                        </div>
                        <div class="faq-item card">
                            <div class="faq-header" id="heading2">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse2" aria-expanded="true" aria-controls="collapse2"><i class="fa fa-caret-right"></i> View Past Appointments</button>
                            </div>

                            <div id="collapse2" class="collapse" aria-labelledby="heading2" data-parent="#accordion">
                                <div class="faq-body">
                                    <ol>
                                        <% if( data.pastappointment != undefined && data.pastappointment.length) { %>
                                            <% data.pastappointment.forEach(function(item){ %>
                                                <div class="col-lg-12">
                                                    <li><b>Patient Name: </b><%= item.PatientName %></li> 
                                                    <b>Patient Phone Number: </b><%= item.PatientPhone %> <br/>
                                                    <b>Appointment Status: </b><%= item.Status %> <br/>
                                                    <b>Appointment Date: </b><%= item.AppointmentDate %> <br/>
                                                    <b>Appointment Time: </b><%= item.AppointmentTime %> <br/>
                                                    <% if(item.PrescriptionId != null) { %>
                                                        <b>Prescription</b> <br/>
                                                        <b>Symptoms: </b><%= item.Symptoms %> <br/> 
                                                        <b>Tests: </b><%= item.Tests %> <br/>
                                                        <b>Medicines: </b><%= item.Medicines %> <br/>
                                                        <b>Remarks: </b><%= item.Remarks %> <br/>
                                                        <b>Doctor Signature: </b> <br/>   
                                                        <img src= "<%= item.DoctorSignature %>" width="100" heigth="100" alt="signature"  > <br/> <br/>
                                                    <% }else{ %>
                                                        <a href="/doctorhome/<%= item.PatientId %>/<%= item.AppointmentId %>/addprescription" class="btn btn-xs btn-confirm">Add Prescription</a> <br/>    
                                                    <% } %>
                                                </div>
                                            <% }); %>
                                    </ol>
                                    <% }else{ %>
                                        <p>
                                            Currently there is no appointment !!!
                                        </p>
                                    <% } %>      

                                </div>
                            </div>
                        </div>

                        <div class="faq-item card">
                            <div class="faq-header" id="heading3">
                                <button class="faq-button collapsed" data-toggle="collapse" data-target="#collapse3" aria-expanded="true" aria-controls="collapse3"><i class="fa fa-caret-right"></i> Update Credentials</button>
                            </div>

                            <div id="collapse3" class="collapse" aria-labelledby="heading3" data-parent="#accordion">
                                <div class="faq-body">
                                    <p>
                                        <a href="/doctorhome/<%= data.user._id%>/edit" class="btn btn-primary btn-large">Update Credentials</a>
                                    </p>                                   
                                </div>
                            </div>
                        </div>
                        <div class="faq-item card">
                        </div>
